{
  "name": "equeum-graphql-server",
  "version": "1.8.3",
  "description": "GraphQL server implementation over Express exporting Equeum specific utilities.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/equeumco/equeum-graphql-server"
  },
  "bugs": {
    "url": "https://github.com/equeumco/equeum-graphql-server/issues"
  },
  "homepage": "https://github.com/equeumco/equeum-graphql-server#readme",
  "scripts": {
    "build": "npx tsc",
    "build:docs": "npx typedoc --out docs/ lib/ && git add ./docs",
    "dev": "npx tsc -w",
    "lint": "npx eslint --ext .ts lib/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Giedrius Tamulaitis <gtamulaitis@equeum.com>",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint & npm run build & npm run build:docs"
    }
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "class-validator": "^0.12.2",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "husky": "^4.3.6",
    "typedoc": "^0.20.13",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@apollo/federation": "^0.20.7",
    "apollo-server": "^2.19.1",
    "apollo-server-express": "^2.19.1",
    "apollo-server-testing": "^2.23.0",
    "dataloader": "^2.0.0",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "graphql-tools": "^7.0.2",
    "graphql-type-json": "^0.3.2",
    "type-graphql": "^1.1.1",
    "useragent": "^2.3.0"
  }
}
